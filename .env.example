# QD-Enhanced Multi-LLM Swarm Agent System Environment Variables

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME="QD-Enhanced Multi-LLM Swarm Agent System"

# HashiCorp Vault Configuration
VAULT_ENDPOINT=https://your-vault-instance.com:8200
VAULT_TOKEN=your-vault-root-token
VAULT_NAMESPACE=your-namespace
VAULT_MOUNT_PATH=secret

# Authentication Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
SESSION_TIMEOUT=86400000
AUTH_ENCRYPTION_KEY=your-32-character-encryption-key

# LLM Provider API Keys (Store these in Vault for production)
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
GOOGLE_API_KEY=your-google-gemini-api-key
BLACKBOX_API_KEY=your-blackbox-api-key
OPENROUTER_API_KEY=sk-or-your-openrouter-api-key
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_ENVIRONMENT=your-pinecone-environment

# Custom LLM Endpoint Configuration (Pre-configured for demo)
CUSTOM_LLM_ENDPOINT=https://oi-server.onrender.com/chat/completions
CUSTOM_LLM_CUSTOMER_ID=cus_Sxr3rlGaox3rKZ
CUSTOM_LLM_AUTHORIZATION=Bearer xxx

# Database Configuration (Optional - for production persistence)
DATABASE_URL=postgresql://username:password@localhost:5432/qd_swarm_db
REDIS_URL=redis://localhost:6379

# QD-Enhanced Features Configuration
QD_MEMORY_CONSOLIDATION_INTERVAL=3600000
QD_REFLECTION_INTERVAL=600000
QD_KNOWLEDGE_TRANSFER_THRESHOLD=0.7
QD_LEARNING_VELOCITY_TARGET=8.0
QD_EMERGENT_BEHAVIOR_DETECTION=true

# Agent Configuration
MAX_AGENTS=7
AGENT_MEMORY_LIMIT=1000
AGENT_QUEUE_LIMIT=10
AGENT_TIMEOUT=300000

# Security Configuration
ENABLE_AUDIT_LOGGING=true
SECURITY_LEVEL=enterprise
CORS_ORIGIN=*
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900000

# Monitoring and Analytics
ENABLE_PERFORMANCE_TRACKING=true
ENABLE_COST_TRACKING=true
ANALYTICS_RETENTION_DAYS=30
LOG_LEVEL=info

# Deployment Configuration
DEPLOYMENT_ENVIRONMENT=development
HEALTH_CHECK_INTERVAL=30000
GRACEFUL_SHUTDOWN_TIMEOUT=10000

# Feature Flags
ENABLE_QD_FEATURES=true
ENABLE_MEMORY_SYSTEMS=true
ENABLE_CAUSAL_ANALYSIS=true
ENABLE_ADAPTIVE_PLANNING=true
ENABLE_KNOWLEDGE_TRANSFER=true
ENABLE_EMERGENT_DETECTION=true

# External Integrations (Optional)
GITHUB_TOKEN=your-github-token
SLACK_WEBHOOK_URL=your-slack-webhook-url
DISCORD_WEBHOOK_URL=your-discord-webhook-url

# Development Configuration
DEBUG_MODE=false
VERBOSE_LOGGING=false
ENABLE_DEV_TOOLS=true